<ParameterList name="SCI">
    <ParameterList name="Parameter">
        <Parameter name="Dimension" type="int" value="3"/>
        <Parameter name="Discretization" type="string" value="P2"/><!--P2-->
        <Parameter name="BC Type" type="string" value="Artery"/><!--Artery, Cube-->
        <Parameter name="RHS Type" type="string" value="Paper"/><!--Constant, Paper, Heart Beat-->
        
        <Parameter name="Mesh Type" type="string" value="unstructured"/> <!--  unstructured -->
        
        <Parameter name="MeanVelocity" type="double" value="2.0"/><!--2d:2.0, Richter 3d: 1.0-->
        <Parameter name="Inflow Ramp" type="double" value="0.0"/><!--2d:2.0, Richter 3d: 1.0-->
        <Parameter name="Volume force" type="double" value="0.0213316"/>
        <Parameter name="Source Type" type="string"   value="surface"/>
        <Parameter name="Model Geometry" type="string" value="Laplace"/>
        
        <Parameter name="SCI" type="bool" value="true"/>
        <Parameter name="Load Stepping" type="bool" value="true"/>
        <Parameter name="Inflow Start Time" type="double" value="1.0"/><!-- Start of diffusion process -->
        <Parameter name="Load Step Size" type="double" value="0.05"/><!-- Length/size of Loadsteps -->
        <Parameter name="Load Ramp End" type="double" value="1.0"/><!-- End of Load Stepping -->
        <Parameter name="External Force" type="bool" value="true"/>
        <Parameter name="Nonlinear External Force" type="bool" value="true"/>
        <Parameter name="Coupling Type" type="string" value="implicit"/>
        <Parameter name="Structure Model" type="string" value="SCI_SMC_Active_Growth_Reorientation"/> <!-- SCI_NH , SCI_SMC_MLCK , SCI_SMC_Active_Growth_Reorientation -->

        <Parameter name="Refinement Level" type="int" value="0"/>
       
        <Parameter name="relNonLinTol" type="double" value="1.0e-6"/>
        <Parameter name="absNonLinTol" type="double" value="1.0e-2"/>
        <Parameter name="updateTol" type="double" value="1.0e-4"/>
        <Parameter name="Combo" type="string" value="OR"/>
        <Parameter name="Use update tol" type="bool" value="false"/>
        <Parameter name="Use WRMS" type="bool" value="false"/>
        
        <Parameter name="MaxNonLinIts" type="int" value="10"/>

        <Parameter name="Criterion" type="string" value="Update"/><!-- Criterion for nonlinear solve, Residual or Update: Residual is the norm of the nonlinear residual; Update is the norm of the update -->
    </ParameterList>

    <ParameterList name="Parameter Diffusion">
        <Parameter name="Viscosity" type="double" value="1.0"/>
        <Parameter name="Density" type="double" value="1000.0"/>
        <Parameter name="m" type="double" value="0."/>
        <Parameter name="D0" type="double" value="6.e-05"/>
        <Parameter name="Symmetric gradient" type="bool" value="true"/>
    </ParameterList>
    
    <ParameterList name="Mesh Partitioner">
        <Parameter name="Contiguous" type="bool" value="true"/>
        
        <Parameter name="Mesh 1 Name" type="string" value="artery_h_0_1.mesh"/> <!-- partialArtery -->
        <Parameter name="Mesh 2 Name" type="string" value="cube_h_1.mesh"/>
        
        <Parameter name="Automatic partition" type="bool" value="false"/> <!-- find ratio for ranks based on the number of elements of each mesh -->
        
        <Parameter name="Mesh 1 fraction ranks" type="int" value="-1"/> <!-- percentage of ranks for mesh 1; if Automatic partition=true this will be ignored; if set below 0 number of ranks will be used -->
        <Parameter name="Mesh 1 number ranks" type="int" value="-1"/><!-- number of ranks for this mesh; if Automatic partition=true this will be ignored; if fraction ranks>=0 this will be ignored -->
        
        <Parameter name="Mesh 2 fraction ranks" type="int" value="-1"/>
        <Parameter name="Mesh 2 number ranks" type="int" value="-1"/>    
    </ParameterList>

    <ParameterList name="Exporter">
        <Parameter name="Write new mesh" type="bool" value="false"/>
        <Parameter name="Export every X timesteps" type="int" value="1"/>
               
        <Parameter name="Export coarse functions" type="bool" value="false"/>
       
        <Parameter name="Name coarse functions block1" type="string" value="phiC"/>
        <Parameter name="Name coarse functions block2" type="string" value="phiD"/>
        <Parameter name="Exclude coarse functions block4" type="bool" value="true"/>
        
    </ParameterList>
    <ParameterList name="General">
        <Parameter name="Mpi Ranks Coarse" type="int" value="0"/>
        <Parameter name="ParaViewExport" type="bool" value="true"/>
        <Parameter name="ParaViewCoarse" type="bool" value="false"/>
        <Parameter name="ExportID" type="int" value="7"/>
        <Parameter name="ParaView export subdomains" type="bool" value="true"/>
        
        <Parameter name="Export Suffix" type="string" value=""/>
        <Parameter name="Export Data" type="bool" value="true"/>
        <Parameter name="Export Extra Data" type="bool" value="false"/>
        <Parameter name="Export drag and lift" type="bool" value="true"/>
        
        <Parameter name="Preconditioner Method" type="string" value="Monolithic"/> <!-- Monolithic, FaCSI, FaCSI-Teko -->
       
        <Parameter name="Only Diagonal" type="bool" value="false"/>

        <Parameter name="Scale mass matrix" type="bool" value="true"/>
        <Parameter name="Set Zeros" type="bool" value="true"/>
        
        <Parameter name="Linearization" type="string" value="Newton"/> <!-- FixedPoint, Newton, Extrapolation-->
    </ParameterList>
    <ParameterList name="Timestepping Parameter">
        
        <Parameter name="BDF" type="int" value="1"/>
        <Parameter name="Class" type="string" value="Multistep"/> <!-- External, Multistep-->
        <Parameter name="Timestepping type" type="string" value="non-adaptive"/><!--non-adaptive,adaptive:not working for BDF2-->
    
        <Parameter name="Final time" type="double" value="1."/>
        <Parameter name="dt" type="double" value=".05"/>
        <Parameter name="beta" type="double" value="0.25"/> <!-- newmark method parameter-->
        <Parameter name="gamma" type="double" value="0.5"/> <!-- newmark method parameter-->
        <ParameterList name="Timestepping Intervalls">
            <Parameter name="Number of Segments" type="int" value="4"/>
            <ParameterList name="1">
                <Parameter name="Start Time" type="double" value="0."/>
                <Parameter name="dt" type="double" value="0.05"/>
            </ParameterList>
             <ParameterList name="2">
                <Parameter name="Start Time" type="double" value="1."/>
                <Parameter name="dt" type="double" value="1."/>
            </ParameterList>
             <ParameterList name="3">
                <Parameter name="Start Time" type="double" value="1001."/>
                <Parameter name="dt" type="double" value="1."/>
            </ParameterList>
             <ParameterList name="4">
                <Parameter name="Start Time" type="double" value="2001."/>
                <Parameter name="dt" type="double" value="0.02"/>
            </ParameterList>
         </ParameterList> 
    </ParameterList>
</ParameterList>

